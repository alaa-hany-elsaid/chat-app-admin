{
  "name": "chat-app-admin",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon src/server.js",
    "server:build": "babel --verbose --out-dir dist --only 'src/server.js,src/backend/' src",
    "client:build": "vite build",
    "build": "npm run server:build && npm run client:build",
    "serve": "vite preview",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@vitejs/plugin-react": "^1.0.2",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "http-proxy-middleware": "^2.0.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "postcss": "^8.4.21",
    "prettier": "2.4.1",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.5.2",
    "tailwindcss": "^3.2.4",
    "vite": "^2.5.10"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@reduxjs/toolkit": "^1.9.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "accesscontrol": "^2.2.1",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^8.3.0",
    "express-validator": "^6.14.3",
    "flowbite": "^1.6.2",
    "flowbite-react": "^0.3.7",
    "jsonwebtoken": "^9.0.0",
    "localforage": "^1.10.0",
    "mysql2": "^3.0.1",
    "node-random-name": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "redis": "^4.6.4",
    "sequelize": "^6.28.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "socket.io-cookie": "^0.0.1",
    "validator": "^13.7.0",
    "yup": "^0.32.11"
  }
}
